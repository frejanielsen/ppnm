set terminal png
set title "SIR model of Denmark, $T_c = 6, T_r = 21$"
set xlabel "t [days]"
set ylabel "N"
set output "SIR_plot2.png"
set tics in
set key outside
plot "SIR_output2.txt" using 1:2 with line linewidth 3 title "$S(t), S0 = 5120201$"\
, "SIR_output2.txt" using 1:3 with line linewidth 3 title "$I(t), I0 = 11162$"\
, "SIR_output2.txt" using 1:4 with line linewidth 3 title "$R(t), R0 = 674637$"

